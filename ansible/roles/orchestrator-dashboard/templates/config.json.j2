{
    "IAM_CLIENT_ID": "{{ dashboard_iam_client_id }}",
    "IAM_CLIENT_SECRET": "{{ dashboard_iam_client_secret }}",
    "IAM_BASE_URL": "{{ dashboard_iam_issuer }}",
    "ORCHESTRATOR_URL": "{{ dashboard_orchestrator_url }}",
    "TOSCA_TEMPLATES_DIR": "/opt/tosca-templates",
    "TOSCA_PARAMETERS_DIR": "/opt/tosca-parameters",
    "TOSCA_METADATA_DIR": "/opt/tosca-metadata",
{% if dashboard_slam_url is defined %}
    "SLAM_URL": "{{ dashboard_slam_url }}",
{% endif %}
{% if dashboard_cmdb_url is defined %}
    "CMDB_URL": "{{ dashboard_cmdb_url }}",
{% endif %}
{% if dashboard_im_url is defined %}
    "IM_URL": "{{ dashboard_im_url }}",
{% endif %}
{% if dashboard_enable_db %}
    "CALLBACK_URL": "https://{{ dashboard_fqdn }}/callback",
    "DB_HOST": "{{ dashboard_db_host }}",
    "DB_USER": "{{ dashboard_db_user }}",
    "DB_PASSWORD": "{{ dashboard_db_password }}",
    "DB_NAME": "{{ dashboard_db_name }}",
    "DB_PORT": "{{ dashboard_db_port }}",
{% endif %}
{% if dashboard_notification_enable %}
    "MAIL_SERVER": "{{ dashboard_mail_server }}",
    "MAIL_PORT": "{{ dashboard_mail_port }}",
    "MAIL_SENDER": "{{ dashboard_mail_sender }}",
{% endif %}
{% if dashboard_enable_vault %}
    "VAULT_URL": "{{ dashboard_vault_url}}",
{% endif %}
    "ADMINS": "{{ dashboard_administrators }}",
    "SUPPORT_EMAIL": "{{ dashboard_support_email }}",
    "ENABLE_ADVANCED_MENU": "{{ dashboard_enable_advanced_menu }}",
    "LOG_LEVEL": "{{ dashboard_log_level }}"
}
